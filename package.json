{
  "name": "strip-debug-loader",
  "version": "1.0.0",
  "description": "Remove console, alert, and debugger statements from code processed by webpack",
  "main": "src/index.js",
  "dependencies": {
    "strip-debug": "1.1.1"
  },
  "devDependencies": {
    "chai": "3.4.1",
    "eslint": "1.10.3",
    "eslint-config-airbnb": "2.1.0",
    "isparta": "4.0.0",
    "mocha": "2.3.4"
  },
  "scripts": {
